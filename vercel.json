{
  "version": 2,
  "builds": [
    {
      "src": "src/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/register",
      "methods": ["POST"],
      "dest": "src/api.js"
    },
    {
      "src": "/api/login",
      "methods": ["POST"],
      "dest": "src/api.js"
    },
    {
      "src": "/api/requests",
      "methods": ["GET"],
      "dest": "src/api.js"
    },
    {
      "src": "/api/enqueue",
      "methods": ["POST"],
      "dest": "src/api.js"
    }
  ]
}

